{
    "$id": "primitive-math-3d-plane",
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "3 dimensional plane schema",
    "properties": {
        "normal": {
            "description": "surface normal of plane",
            "$ref": "file:vector.json"
        },
        "origin": {
            "description": "origin of normal vector to the plane",
            "$ref": "file:point.json"
        }
    },
    "required": [
        "normal",
        "origin"
    ]
}
