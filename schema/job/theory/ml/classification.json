{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "classification method schema",
    "properties": {
        "type": {
            "enum": [
                "classification"
            ]
        },
        "subtype": {
            "enum": [
                "logistic_regression",
                "neural_network",
                "decision_tree"
            ]
        },
        "precision": {
            "$ref": "file:classification/precision.json"
        },
        "data":{
            "$ref":"file:model_parameters/model_parameters.json"
        }
    },
    "required": [
        "type",
        "subtype",
        "precision"
    ]
}
