{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Python Programing Language Schema",
    "type": "object",
    "properties": {
        "name": {
            "enum": [
                "python"
            ]
        },
        "flavor": {
            "enum": [
                "python2",
                "python3"
            ]
        },
        "summary": {
            "enum": [
                "Python Programing Language"
            ]
        },
        "version": {
            "enum": [
                "2",
                "3"
            ]
        },
        "exec": {
            "enum": [
                "python2",
                "python3"
            ]
        }
    }
}
