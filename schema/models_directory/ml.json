{
    "schemaId": "models-directory-ml",
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "machine learning model schema",
    "description": "machine learning model type/subtype schema",
    "allOf": [
        {
            "$ref": "../model.json"
        }
    ],
    "properties": {
        "type": {
            "enum": [
                "ml"
            ]
        },
        "subtype": {
            "enum": [
                "re"
            ]
        },
        "method": {
            "oneOf": [
                {
                    "$ref": "../methods_directory/regression.json"
                }
            ]
        }
    }
}
