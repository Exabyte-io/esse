{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "title": "pseudopotential method schema", 
    "properties": {
        "type": {
            "enum": [
                "pseudopotential"
            ]
        }, 
        "subtype": {
            "enum": [
                "us", 
                "nc", 
                "paw"
            ]
        }, 
        "precision": {
            "$ref": "file:pseudopotential/precision.json"
        }, 
        "data": {
            "properties": {
                "dateSet": {
                    "$ref": "file:pseudopotential/dataset.json"
                }
            }
        }
    }
}
