{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "valence orbital occupancy electronic descriptor schema",
    "description": "s,p,d,f orbital contributions to valence",
    "properties": {
        "name": {
            "description": "orbital name",
            "type": "string",
