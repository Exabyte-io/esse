{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "atomization energy schema",
    "allOf": [
        {
            "$ref": "file:../../primitive/scalar.json"
        }
    ],
    "properties": {
        "description": "energy of a free atom (or diatom) in the case of O, F, Br, I, H, & Cl",
        "name": {
            "enum": [
