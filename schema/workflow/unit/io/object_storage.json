{
    "schemaId": "workflow-unit-io-object-storage",
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "object_storage io schema",
    "type": "object",
    "allof": [
        {
            "$ref": "../../unit.json"
        }
    ],
    "properties": {
        "pathname": {
            "description": "Name of the directory to contain the file when it is downloaded. If empty, the current directory is used",
            "type": "string",
            "default": ""
        },
        "basename": {
            "description": "Basename the file should have when it is downloaded",
            "type": "string"
        },
        "objectData": {
            "$ref": "../../../core/reusable/object_storage_container_data.json"
        },
        "overwrite": {
            "description": "if a file with the same filename already exists, whether to overwrite the old file",
            "type": "boolean",
            "default": false
        }
    },
    "required": [
        "objectData",
        "basename"
    ]
}
