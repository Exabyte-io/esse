{
    "schemaId": "workflow-unit-download-from-object-storage",
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "download-from-object-storage io schema",
    "type": "object",
    "allof": [
        {
            "$ref": "../../unit.json"
        }
    ],
    "properties": {
        "filename": {
            "description": "name given to the file when it is downloaded",
            "type": "string"
        },
        "objectData": {
            "$ref": "../../../core/reusable/object_storage_container_data.json"
        },
        "overwrite": {
            "description": "if a file with the same filename already exists, whether to overwrite the old file",
            "type": "boolean",
            "default": false
        }
    },
    "required": [
        "filename",
        "objectData"
    ]
}
