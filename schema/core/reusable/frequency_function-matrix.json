{
    "$id": "core/reusable/frequency-function-matrix",
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "",
    "description": "Schema for a function of frequency yielding a nx3 matrix",
    "type": "object",
    "properties": {
        "frequencies": {
            "description": "Frequencies",
            "type": "array",
            "items": {
                "type": "number"
            }
        },
        "components": {
            "description": "Matrix with 3 columns, e.g. x, y, z",
            "type": "array",
            "items": {
                "$id": "core/primitive/array-of-3-numbers",
                "$schema": "http://json-schema.org/draft-04/schema#",
                "title": "array of 3 number elements schema",
                "type": "array",
                "minItems": 3,
                "maxItems": 3,
                "items": {
                    "type": "number"
                }
            }
        }
    }
}