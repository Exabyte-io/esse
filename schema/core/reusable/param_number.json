{
    "schemaId": "core-reusable-param-number",
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "number parameter schema",
    "allOf": [
        {
            "$ref": "../primitive/scalar.json"
        }
    ],
    "properties": {
        "units": {
            "description": "Unit of number parameter",
            "type": "string"
        },
        "precision": {
            "description": "Whether parameter affects precision",
            "type": "boolean"
        }
    },
    "required": [
        "precision"
    ]
}
